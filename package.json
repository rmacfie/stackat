{
  "name": "stackat",
  "description": "Simple HTTP server for NodeJS with full support for async/await and TypeScript",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Robert Macfie <robert@macfie.se>",
  "homepage": "https://github.com/rmacfie/stackat",
  "repository": "github:rmacfie/stackat",
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "*.js",
    "*.d.ts",
    ".editorconfig",
    ".jsbeautifyrc",
    "tsconfig.json",
    "tslint.json"
  ],
  "scripts": {
    "clean": "rimraf ./*.{js,d.ts}",
    "prebuild": "run-s clean",
    "build": "tsc",
    "preprepare": "run-s clean",
    "prepare": "tsc",
    "test": "jest",
    "watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.6",
    "jest": "^22.4.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.1",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.(test|spec)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
